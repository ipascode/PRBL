
	if ( $("#job-<%=@job.id%>").text() == "Not Started") {	
	$("#job-<%=@job.id%>").text('<%= @job.status%>')
	$("#timestart-<%=@job.id%>").text('<%= @job.timestarted.strftime("%X %p")%>')
	}

	else{
	$("#job-<%=@job.id%>").text('<%=@job.status%>')
